<div class="card-container">
  <% @restaurants.each do |restaurant| %>
    <%# Et pour chaque "restaurant", j'affiche les infos (name et address), la possibilité de l'ajouter à une collection via l'icon %>
    <%# et le lien cers la show du restaurant %>
    <div class="card">
<%# https://lh3.googleusercontent.com/places/ANXAkqFE188bvuUs7cu_JFArdxK7kuCOVoyM03gu5hU9b8B-Cp-Ty2qwoZ4UxuOWrbifCdGKemjIQuqo7qnLs4BtxdqVLxbnPnr-18U=s4800-w1024-h768" %>


      <div class="card-body">
        <h5 class="card-title"><%= restaurant[1][0]["displayName"]["text"] %></h5>
        <p class="card-text"><%= restaurant[1][0]["formattedAddress"] %></p>
        <p class="card-text">Rating: <%= restaurant[1][0]["rating"] %></p>
        <%# <%= link_to "View Restaurant", google_restaurant_path(restaurant["place_id"]), class:"card-btn btn-primary" %>
      </div>
    </div>
  <% end %>
</div>
